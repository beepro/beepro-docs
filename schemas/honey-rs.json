  "type": "object",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "string",
      "title": "Honey Resource ID"
    },
    "dance": {
      "$id": "/properties/dance",
      "type": "object",
      "properties": {
        "url": {
          "$id": "/properties/dance/properties/url",
